{
  "name": "ss-intermediary",
  "version": "1.0.0",
  "description": "Video rendering intermediary platform",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "test": "turbo run test",
    "db:generate": "cd apps/web && npm run db:generate",
    "db:push": "cd apps/web && npm run db:push",
    "db:migrate": "cd apps/web && npm run db:migrate"
  },
  "devDependencies": {
    "turbo": "^1.13.0"
  },
  "packageManager": "npm@10.0.0"
}