# Database
DATABASE_URL=postgresql://user:password@localhost:5432/ss_inter

# Shotstack API
SHOTSTACK_API_KEY=your-shotstack-api-key
SHOTSTACK_API_URL=https://api.shotstack.io/v1

# Redis
REDIS_URL=redis://localhost:6379

# Web Service
WEB_SERVICE_URL=http://localhost:3000

# JWT Secret
JWT_SECRET=your-jwt-secret-key

# API Configuration
API_HOST=0.0.0.0
API_PORT=8001
ENVIRONMENT=development

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=3600

# Google Cloud Storage
GCS_BUCKET=your-gcs-bucket-name
GCS_PATH_PREFIX=videos
GCS_ACL=publicRead
GOOGLE_APPLICATION_CREDENTIALS=/app/gcp-credentials.json